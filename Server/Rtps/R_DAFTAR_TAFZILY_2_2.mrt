  <Script>using System;
using System.Drawing;
using System.Windows.Forms;
using System.Data;
using Stimulsoft.Controls;
using Stimulsoft.Base.Drawing;
using Stimulsoft.Report;
using Stimulsoft.Report.Dialogs;
using Stimulsoft.Report.Components;

namespace Reports
{
	public class Report : Stimulsoft.Report.StiReport
	{
		public Report()        {
			this.InitializeComponent();
		}

		#region StiReport Designer generated code - do not modify
		#endregion StiReport Designer generated code - do not modify
		
		public string ConvertNumberToWords(decimal number)
		{
			string[] unitsMap = { "صفر", "یک", "دو", "سه", "چهار", "پنج", "شش", "هفت", "هشت", "نه", "ده", "یازده", "دوازده", "سیزده", "چهارده", "پانزده", "شانزده", "هفده", "هجده", "نوزده" };
			string[] tensMap = { "", "", "بیست", "سی", "چهل", "پنجاه", "شصت", "هفتاد", "هشتاد", "نود" };
			string[] hundredsMap = { "", "صد", "دویست", "سیصد", "چهارصد", "پانصد", "ششصد", "هفتصد", "هشتصد", "نهصد" };
			string[] largeNumbersMap = { "", "هزار", "میلیون", "میلیارد", "تریلیون", "کوادریلیون", "کوینتیلیون", "سکستیلیون", "سپتیلیون", "اکتیلیون", "نانیلیون", "دسیلیون", "تیلیارد" };

			if (number == 0)
				return "صفر";

			if (number &lt; 0)
				return "منفی " + ConvertNumberToWords(Math.Abs(number));

			string words = "";
			int groupIndex = 0;

			while (number &gt; 0)
			{
				int group = (int)(number % 1000);
				if (group &gt; 0)
				{
					string groupWords = ConvertGroupToWords(group, unitsMap, tensMap, hundredsMap);
					if (groupIndex &gt; 0)
						groupWords += " " + largeNumbersMap[groupIndex];

					words = groupWords + (string.IsNullOrEmpty(words) ? "" : " و ") + words;
				}

				number = Math.Floor(number / 1000);
				groupIndex++;
			}

			return words.Trim();
		}

		private string ConvertGroupToWords(int number, string[] unitsMap, string[] tensMap, string[] hundredsMap)
		{
			string words = "";

			if ((number / 100) &gt; 0)
			{
				words += hundredsMap[number / 100] + " ";
				number %= 100;
			}

			if (number &gt; 0)
			{
				if (number &lt; 20)
					words += unitsMap[number];
				else
				{
					words += tensMap[number / 10];
					if ((number % 10) &gt; 0)
						words += " و " + unitsMap[number % 10];
				}
			}

			return words.Trim();
		}
		
	}
}
</Script>
